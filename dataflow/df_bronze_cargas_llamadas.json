{
	"name": "df_bronze_cargas_llamadas",
	"properties": {
		"folder": {
			"name": "Bronze"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_llamadas_tigo_csv",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sql_llamadas_tigo",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          DESCRIPCION as string,",
				"          TIPO as string,",
				"          NUMERO_ORIGEN as string,",
				"          NUMERO_DESTINO as string,",
				"          FECHA as string,",
				"          HORA as string,",
				"          DURACION as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          descripcion as string,",
				"          tipo as string,",
				"          numero_origen as string,",
				"          numero_destino as string,",
				"          fecha as string,",
				"          hora as string,",
				"          duracion as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}